{
  "expo": {
    "name": "ACTADSI",
    "scheme": "ACTADSI",
    "slug": "ACTADSI",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/art-dir-1.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/art-dir-2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kishorear.ACTADSI",
      "buildNumber": "1.0.0",
      "icon": "./assets/art-dir-1.png",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan QR codes."
      },
      "config": {
        "usesNonExemptEncryption": false
      },
      "minimumOsVersion": "12.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/art-dir-1.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.kishorear.ArtCircle"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a5d22a13-89b7-4c5f-9414-5e35bc1a508e"
      }
    }
  }
}
